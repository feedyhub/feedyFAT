<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FeedyWPF"
        xmlns:Models="clr-namespace:FeedyWPF.Models" x:Class="FeedyWPF.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800" >
    <Grid>
        <TabControl x:Name="tabControl" ItemsSource="{Binding}" HorizontalAlignment="Stretch"  Margin="10,10,0,0" SelectionChanged="tabControl_SelectionChanged" VerticalAlignment="Stretch" />
    </Grid>
   
</Window>

